# ========== ./Common/Game/AI/FollowState.cpp ==========
#: ./Common/Game/AI/FollowState.cpp:
msgid "Your craft %1% can't follow %2% because it won't fit through a wormhole or gate."
msgstr "La tua nave %1% non può seguire l' %2% perché non passerà attraverso un Wormhole o un portale."

#: ./Common/Game/AI/FollowState.cpp:
msgid "One of your crafts can't follow its target %s because it won't fit through a wormhole or gate."
msgstr "Una delle tue navi non può seguire il suo obiettivo %s perché non passerà attraverso un Wormhole o un portale."

#: ./Common/Game/AI/FollowState.cpp:
msgid "Your craft %1% has lost track of its target %2%."
msgstr "La tua nave %1% ha perso le tracce del suo bersaglio %2%."

#: ./Common/Game/AI/FollowState.cpp:
msgid "One of your crafts can't follow its target %s because it can't jump far enough."
msgstr "Una delle vostre navi non può seguire il suo obiettivo perché non riesce a saltare abbastanza lontano."

#: ./Common/Game/AI/FollowState.cpp:
msgid "One of your crafts can't follow its target %s because it won't fit through a wormhole or gate."
msgstr "Una delle vostre navi non può seguire il suo obiettivo perché non può passare attraverso un wormhole o un portale."

#: ./Common/Game/AI/FollowState.cpp:
msgid "One of your crafts has lost track of its target %s."
msgstr "Una delle tue navi ha perso le tracce del suo bersaglio %s."


# ========== ./Common/Game/AI/JumpState.cpp ==========
#: ./Common/Game/AI/JumpState.cpp:
msgid "%1% has not jumped for 5 minutes because it is waiting for an escorting ship."
msgstr "%1% non salta da 5 minuti perché è in attesa di una nave di scorta."

#: ./Common/Game/AI/JumpState.cpp:
msgid "%1% can't perform a hyperspace jump because it doesn't have enough energy."
msgstr "%1% non può eseguire un salto nell'iperspazio perché non ha abbastanza energia."


# ========== ./Common/Game/Alliance.cpp ==========
#: ./Common/Game/Alliance.cpp:
msgid "Invite other players to the alliance."
msgstr "Invita altri giocatori all'alleanza."

#: ./Common/Game/Alliance.cpp:
msgid "Kick other members ranked lower than you from the alliance."
msgstr "Caccia dall'alleanza gli altri membri che sono classificati più in basso di te."

